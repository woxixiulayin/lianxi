# EASY-INSTALL-ENTRY-SCRIPT: 'livereload==2.4.0','console_scripts','livereload'
__requires__ = 'livereload==2.4.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('livereload==2.4.0', 'console_scripts', 'livereload')()
    )
